void work()
{
	printf("%d %d\n",rand()%50,rand()%50);
	printf("%d\n",rand()%100);
	n=rand()%5;m=rand()%5;
	printf("%d %d\n",n,m);
	for (i=1;i<=n+m;i++) printf("%d %d %d\n",rand()%100,rand()%20+1,rand()%20+1);

}
int main()
